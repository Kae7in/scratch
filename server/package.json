{
  "name": "@scratch/server",
  "version": "0.0.0",
  "main": "src/server.ts",
  "type": "module",
  "scripts": {
    "start": "pnpm run build && NODE_ENV=production node dist/server.js",
    "prebuild": "pnpm run build",
    "build": "tsc && tsc-alias",
    "watch": "concurrently \"tsc -w --preserveWatchOutput\" \"tsc-alias -w\"",
    "dev": "concurrently -k -p \"[{name}]\" -n \"TypeScript,App\" -c \"yellow.bold,cyan.bold\" \"pnpm:watch\" \"NODE_ENV=development nodemon --stack-size=4096 --delay 250ms dist/server.js\"",
    "lint": "prettier --check . --log-level warn",
    "lint:fix": "prettier --write .",
    "test": "NODE_ENV=testing vitest --pool=forks",
    "test:ui": "NODE_ENV=testing vitest --ui --pool=forks",
    "test:run": "NODE_ENV=testing vitest run --pool=forks",
    "coverage": "NODE_ENV=testing vitest run --coverage --pool=forks",
    "db:reset": "prisma migrate reset",
    "db:generate:sql": "prisma generate --sql && pnpm run build",
    "db:reset:test": "dotenv -e .env.test -- prisma migrate reset",
    "db:migrate:deploy": "prisma migrate deploy",
    "db:migrate:dev": "prisma migrate dev",
    "db:migrate:test": "dotenv -e .env.test -- prisma migrate dev",
    "db:push": "prisma db push",
    "db:seed": "tsx prisma/seed.ts",
    "db:generate": "prisma generate && pnpm run build",
    "format": "prisma format",
    "studio": "prisma studio"
  },
  "dependencies": {
    "@ai-sdk/openai": "^1.0.4",
    "@cerebras/cerebras_cloud_sdk": "^1.16.0",
    "@fastify/compress": "^7.0.3",
    "@fastify/cookie": "^9.3.1",
    "@fastify/cors": "^9.0.1",
    "@fastify/csrf-protection": "^6.4.1",
    "@fastify/error": "^3.4.1",
    "@fastify/formbody": "7",
    "@fastify/jwt": "^8.0.1",
    "@fastify/multipart": "^8.3.0",
    "@fastify/sensible": "^5.6.0",
    "@fastify/swagger": "^8.15.0",
    "@fastify/swagger-ui": "^3.1.0",
    "@ianvs/prettier-plugin-sort-imports": "^4.4.1",
    "@prisma/client": "6.2.1",
    "@types/cookie": "^0.6.0",
    "ai": "^4.0.4",
    "dotenv": "^16.4.5",
    "fastify": "^4.28.1",
    "fastify-cli": "^6.3.0",
    "fastify-plugin": "^4.5.1",
    "fastify-type-provider-zod": "^1.2.0",
    "jsonwebtoken": "^9.0.2",
    "lodash-es": "^4.17.21",
    "luxon": "^3.5.0",
    "openai": "^4.68.4",
    "pg": "^8.13.1",
    "prettier": "^3.4.2",
    "prettier-plugin-svelte": "^3.3.3",
    "prettier-plugin-tailwindcss": "^0.6.10",
    "qs": "^6.13.0",
    "vitest": "^1.6.0",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@hey-api/openapi-ts": "^0.60.1",
    "@types/jsonwebtoken": "^9.0.7",
    "@types/lodash": "^4.17.12",
    "@types/lodash-es": "^4.17.12",
    "@types/luxon": "^3.4.2",
    "@types/node": "^20.17.12",
    "@types/pg": "^8.11.10",
    "@types/qs": "^6.9.16",
    "@types/supertest": "^6.0.2",
    "concurrently": "^8.2.2",
    "cookie": "^1.0.1",
    "nodemon": "^3.1.7",
    "pino-pretty": "^11.3.0",
    "prisma": "^6.2.1",
    "ts-node": "^10.9.2",
    "tsc-alias": "^1.8.10",
    "tsconfig-paths": "^4.2.0",
    "tsx": "^4.19.2",
    "typescript": "^5.7.3"
  }
}
